@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .
@prefix sh:     <http://www.w3.org/ns/shacl#> .
@prefix brick:  <https://brickschema.org/schema/Brick#> .
@prefix bacnet: <http://data.ashrae.org/bacnet/2020#> .
@prefix ref:    <https://brickschema.org/schema/Brick/ref#> .
@prefix :       <urn:dr_constraints/> .

:
    a owl:Ontology .

:damper
    a              sh:NodeShape ;
    sh:targetClass brick:Damper ;
    sh:message     "need a Damper" ;
    sh:property    [ sh:path                brick:hasPoint ;
                     sh:message             "need Damper_Position_Sensor" ;
                     sh:qualifiedValueShape [ sh:class brick:Damper_Position_Sensor ; ] ;
                     sh:qualifiedMinCount   1 ;
                     sh:qualifiedMaxCount   1 ; ] ;
    sh:property    [ sh:path                brick:hasPoint ;
                     sh:message             "need Damper_Position_Command" ;
                     sh:qualifiedValueShape [ sh:class brick:Damper_Position_Command ; ] ;
                     sh:qualifiedMinCount   1 ;
                     sh:qualifiedMaxCount   1 ; ] ;
.


